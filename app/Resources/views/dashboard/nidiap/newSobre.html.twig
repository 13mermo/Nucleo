{% extends '/dashboard/template.html.twig' %}

{% block title %}Dashboard Sobre{% endblock %}

{% block body %}
    <div class="fundo">
        <h2>Informar Sobre Nidiap</h2>
        {{ form_start(form) }}
            <h3>Titulo:</h3>
            {{ form_widget(form.title) }}
